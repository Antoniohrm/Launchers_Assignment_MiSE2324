function [ders] = exoAtmDer(t, state, Rocket, Mission)

% Separate state into components to improve code readability

r = state(1:3);
v = state(4:6);
pr    = X(7:9);
pv    = X(10:12);

weight = -1 * state(13) * (Mission.mu / (norm(r)^3)) * r;
thracc = (Rocket.mdot(Rocket.actstage) * Rocket.isp(Rocket.actstage) * Mission.g) / state(13);
vdot3 = pv / norm(pv);


ders(1:3) = r;
ders(4:6) = weight + (thracc * )
ders(7:9) = [];
ders(10:12) = [];
ders(13) = Rocket.mdot(Rocket.actstage);